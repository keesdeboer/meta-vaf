rootfs               /                    auto       defaults,ro           1  1
proc                 /proc                proc       defaults              0  0
tmpfs                /dev                 tmpfs      defaults              0  0
devpts               /dev/pts             devpts     mode=0620,gid=5       0  0
tmpfs                /tmp                 tmpfs      defaults,size=64m     0  0
/dev/mmcblk0p3       /application         ext4       defaults,ro           0  0
tmpfs                /var                 tmpfs      defaults,size=32m     0  0
/dev/mmcblk0p4       /database            ext4       defaults,sync         0  2
tmpfs                /database/log        tmpfs      defaults,size=32m     0  0
